{{#each this}}
<div class="photo-card">
<a href = "{{webformatURL}}">
  <img src="{{largeImageURL}}" alt="{{tags}}"/>
  <div class="info">
    <p class="info-item">
      <b>Likes</b><span>{{likes}}</span>
    </p>
    <p class="info-item">
      <b>Views</b><span>{{views}}</span>
    </p>
    <p class="info-item">
      <b>Comments</b><span>{{comments}}</span>
    </p>
    <p class="info-item">
      <b>Downloads</b><span>{{downloads}}</span>
    </p>
  </div>
  </a>
</div>
{{/each}}